{
  "name": "like/notification-queue-bundle",
  "description": "notification queue bundle",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Like\\NotificationQueueBundle\\": "src/"
    }
  },
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "doctrine/annotations": "^1.0",
    "phpdocumentor/reflection-docblock": "^5.2",
    "ramsey/uuid": "^4.2",
    "symfony/console": "5.4.*",
    "symfony/dotenv": "5.4.*",
    "symfony/flex": "^1.3.1",
    "symfony/framework-bundle": "5.4.*",
    "symfony/messenger": "^5.4",
    "symfony/property-access": "5.4.*",
    "symfony/property-info": "5.4.*",
    "symfony/runtime": "5.4.*",
    "symfony/serializer": "5.4.*",
    "symfony/yaml": "5.4.*"
  },
  "config": {
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  }
}
